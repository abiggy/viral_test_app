<%= form_for(@twitter_account) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :name, "Twitter username:" %>
  <%= f.text_field :name %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Add Twitter account", class: "btn btn-large btn-primary" %>
<% end %>
